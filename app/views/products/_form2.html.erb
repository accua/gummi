<%= render 'layouts/errors', :object => @product %>

<%= form_for @product do |f| %>
  <div class="input-field col s6">
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  <div class="input-field col s6">
    <%= f.label :cost %>
    <%= f.text_field :cost %>
  </div>
  <div class="input-field col s6">
    <%= f.label :country %>
    <%= f.text_field :country %>
  </div>
  <br>
  <%= f.submit :class => 'btn' %>
<% end %>
